  "keywords": ["laravel", "mail", "email", "monitoring", "debugging"],
  "license": "MIT",
  "authors": [
    {
      "name": "Srinath Reddy Dudi",
      "email": "srinathreddydudi@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1.0",
